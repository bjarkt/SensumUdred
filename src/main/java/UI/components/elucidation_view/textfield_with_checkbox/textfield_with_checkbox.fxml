<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXCheckBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="textField_container" style="-fx-background-color: white;" stylesheets="@textfield_with_checkbox.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.components.elucidation_view.textfield_with_checkbox.TextFieldWithCheckboxController">
   <children>
      <HBox alignment="CENTER_LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TextField fx:id="textField" onKeyPressed="#onTextEnter" styleClass="elucidationView_simpleInputField" HBox.hgrow="ALWAYS" />
            <JFXCheckBox fx:id="checkBox" contentDisplay="GRAPHIC_ONLY" onMouseClicked="#clickCheckBox">
               <HBox.margin>
                  <Insets left="16.0" />
               </HBox.margin>
            </JFXCheckBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
